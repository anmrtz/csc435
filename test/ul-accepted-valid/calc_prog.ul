
int sub(int l, int r) {
    return (l - r);
}

int add(int l, int r) {
    return (l + r);
}

int mult(int l, int r) {
    return (l * r);
}

boolean isEqual(int l, int r) {
    return (l == r);
}

boolean isLessThan(int l, int r) {
    return (l < r);
}

void printFibonacci(int n) {
    int i;
    int[3] f;

    f[0] = 1;
    f[1] = 2;

    i = 2;

    while (i < n) {
        f[i] = f[i-1] + f[1-2];
        println f[i];

        f[i - 2] = f[i - 1];
        f[i - 1] = f[i];
        
        i = i + 1;
    }
}

boolean isSeven(int i) {
    if (i == 7) {
        return true;
    } else {
        return false;
    }
}

